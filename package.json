{
  "name": "unlibjs",
  "version": "0.0.1",
  "description": "jff utility library",
  "type": "module",
  "source": "src/library.js",
  "main": "dist/unlib.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "lint": "eslint src",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "jest-environment-node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm lint && npm test",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yaruson/unlibjs.git"
  },
  "author": "Nikolai Kim",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/yaruson/unlibjs/issues"
  },
  "homepage": "https://github.com/yaruson/unlibjs#readme",
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "microbundle": "^0.12.2"
  }
}
